{
    "name": "spiffy/spiffy-navigation",
    "description": "Zend Framework 2 Module that aims to replace Zend\\Navigation",
    "type": "library",
    "keywords": [
        "module",
        "zf2"
    ],
    "homepage": "http://www.github.com/spiffyjr/spiffy-navigation/",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        }
    ],

    "require": {
        "php": "^7.2",
        "zendframework/zend-console": "^2.8",
        "zendframework/zend-eventmanager": "^3.2",
        "zendframework/zend-form": "^2.14",
        "zendframework/zend-modulemanager": "^2.8",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-permissions-acl": "^2.7",
        "zendframework/zend-permissions-rbac": "^3.0",
        "zendframework/zend-serializer": "^2.9",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-stdlib": "^3.2",
        "zendframework/zend-uri": "^2.7",
        "zendframework/zend-view": "^2.11"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zend-config": "~2.2"
    },

    "suggest": {
        "zend/zend-config": "To use the ConfigProvider (yaml, xml support)"
    },

    "autoload": {
        "psr-0": {
            "SpiffyNavigation": "src/",
            "SpiffyNavigationTest": "test/"
        }
    }
}
